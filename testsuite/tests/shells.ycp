{

    include "testsuite.ycp";
    import "Users";

    map READ = $[ "etc" : $["shells" : ["/bin/ash", "/bin/bash", "/bin/bash1", "/bin/csh", "/bin/false", "/bin/ksh", "/bin/sh"]]];

    TEST(``(Users::GetAllShells (`items)), [READ, $[], $[]], 0);
    DUMP ("==========================================================");
    TEST(``(Users::GetAllShells (`stringlist)), [READ, $[], $[]], 0);

}
