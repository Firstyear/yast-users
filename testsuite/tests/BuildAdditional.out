Read	.target.size "/etc/install.inf" -1
Read	.target.tmpdir "/tmp/"
Read	.target.tmpdir "/tmp/"
Execute	.target.bash "/usr/lib/YaST2/bin/version" 0
Read	.sysconfig.language.RC_LANG ""
Read	.sysconfig.language.DEFAULT_LANGUAGE ""
Read	.target.yast2 "lang2iso.ycp" nil
Read	.target.size "/etc/sysconfig/language" -1
Read	.sysconfig.console.CONSOLE_FONT ""
Read	.sysconfig.console.CONSOLE_SCREENMAP ""
Read	.sysconfig.console.CONSOLE_UNICODEMAP ""
Read	.sysconfig.console.CONSOLE_MAGIC ""
Read	.sysconfig.console.CONSOLE_ENCODING ""
Read	.sysconfig.language.RC_LANG ""
Write	.target.string "/tmp/additional_userlist" "aa" true
Write	.target.string "/tmp/additional_moreusers" "hh" true
Write	.target.ycp "/tmp/additional_usernames" $["ldap":["ldap1", "ldap2"], "local":["aa", "bb", "hh"]] true
Execute	.target.bash "/usr/lib/YaST2/bin/build_additional_users.pl /tmp" 0
Read	.target.ycp "/tmp/additional_more.ycp" [`item (`id ("aa"), "aa", "", true)]
Read	.target.ycp "/tmp/additional_ulist.ycp" [`item (`id ("aa"), "aa", "", true)]
Return	$[`more:[`item (`id ("aa"), "aa", "", true)], `ulist:[`item (`id ("aa"), "aa", "", true)]]
