/*
 * File:
 *   etc_default_useradd.scr
 * Summary:
 *   SCR Agent for reading/writing /etc/defaults/useradd
 * Access:
 *   r/w
 *
 * $Id$
 */
.etc.default.useradd

`ag_ini(
  IniAgent(
    "/etc/default/useradd",
    $[ "options" : ["ignore_case", "prefer_uppercase", "global_values", "flat"],
       "comments" : [ "^#.*", "^[ \t]*$", ],
       "params" : [
		 $[ "match" : [ "^[ \t]*([^=]*[^ \t=])[ \t]*=[ \t]*(.*[^ \t]|)[ \t]*$" , "%s=%s"],],
       ]
	]
    ))
